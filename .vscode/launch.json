    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "Generate MangaPlus app secret",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": ["sources", "mp_gen_api_key"]
        },
        {
            "name": "Use MangaPlus web api",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": ["config", "set", "-k", "mangaplus.use_app_api", "-v", "false"]
        },
        {
            "name": "Use MangaPlus app api",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": ["config", "set", "-k", "mangaplus.use_app_api", "-v", "true"]
        },
        {
            "name": "Get config info",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": ["config", "info"]
        },
        {
            "name": "Clear cache",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": ["clear", "-c"]
        },
        {
            "name": "Update ComicInfo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "console": "integratedTerminal",
            "args": ["inline", "anilist", "update", "-p", "D:\\Boruto Test\\Boruto Naruto Next Generations"]
        },
    ]
}